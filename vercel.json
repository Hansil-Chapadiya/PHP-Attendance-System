{
    "version": 2,
    "functions": {
        "api/**/*.php": {
            "runtime": "nodejs14.x"
        }
    },
    "routes": [
        {
            "src": "/student/login",
            "dest": "/api/stud_login.php"
        },
        {
            "src": "/student/register",
            "dest": "/api/stud_register.php"
        },
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}